class Node {
	public object Value;
    public Node Left;
    public Node Right;

    public int CalcHeight() {
		return 1 + Math.Max(
		    Left != null ? Left.CalcHeight() : 0,
		    Right != null ? Right.CalcHeight() : 0
	    );
	}
}

class Math {
	public static int Max(int a, int b) {
		return a > b ? a : b;
	}
}

