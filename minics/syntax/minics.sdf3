module minics

imports
  Common
  classes
  statements
  expressions

context-free start-symbols
  Start

context-free sorts
  Start MainClassDecl

context-free syntax

  Start.Program = <
    <{UsingDecl "\n"}*>
    <MainClassDecl>
    <{ClassDecl "\n"}*>
  >

  MainClassDecl.MainClass = <
    public class <ID>
    {
      public static void Main()
      {
        <{Stmt "\n"}*>
      }
    }
  >

template options

  ID = keyword {reject}
  keyword -/- [A-Za-z0-9]

